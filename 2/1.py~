a = 1
b = 3

if b <= a:
    print('<')
    print('as')
    if b == a:
        print('==')
        print('as')
else:
    print('>')
    print('as')
d = [1, 'asdas', None, [1, 2, 3], True, (1, 2, 3), 2.6]
for el in d:
    print(el)
print('#'*100)
print(el)
i = 5
while i > 0:
    i -= 1
    print(i)

max_retry = 5
for i in range(5):
    #i -= 1
    print('retry #{}'.format(max_retry-i ))
#1/0
try:
    #d[123]
    1=0
except ZeroDivisionError as e:
    print('zzzero')
    print(type(e))
except Exception as e:
    print(e)
    print(type(e))
finally:
    print('final')
print('!'*100)
